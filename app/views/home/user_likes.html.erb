        isGoogleLoaded = true;
        console.debug("Google Loaded");
        if(isFBLoaded){
            console.debug("Google & FB Loaded");
            getuserlikes(<%=@photo_id%>);
        }
    });
    $(document).bind("onFacebookReady",function(){

   //     $(document).bind("onGoogleReady", function(){
        FB.getLoginStatus(
                function(response) {
                    if (response.status === "connected") {
                        console.debug("FB Loaded");
                        isFBLoaded = true;
                        if(isGoogleLoaded){
                            console.debug("Google & FB Loaded");
                            getuserlikes(<%=@photo_id%>);
                        }

                        //$(document).trigger("onGoogleReady");
                    } else if (response.status === "not_authorized") {
                        //user is currently logged in to facebook, but has not authorized this app
                    } else {
                        //user is not logged in to facebook
                    }
                }
        );
     //   });

    });

</script>